{% extends '@!EasyAdmin/page/content.html.twig' %}

{% block main %}

    <h2>Внутренний ID товара - {{ productId }}</h2>
    <h3> Наименование товара - {{ productName }}</h3>
    <ul>
        <table>

            <tr><td> Цена {{ productPrice }} руб.</td></tr>
            <tr><td> Артикул товара - {{ productSku }}</td></tr>
            <tr><td>Продавец {{ Seller }}</td></tr>
            <tr><td> Создан {{ productCreatedAt }}</td></tr>

        </table>
    </ul>
{% endblock main %}
